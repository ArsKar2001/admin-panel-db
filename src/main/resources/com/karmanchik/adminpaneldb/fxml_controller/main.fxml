<VBox prefHeight="600.0" prefWidth="900.0" stylesheets="@../../../../styles/mainStyle.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.karmanchik.adminpaneldb.fxml_controller.MainController">
    <SplitPane dividerPositions="0.25" focusTraversable="true" VBox.vgrow="ALWAYS">
        <AnchorPane maxWidth="400.0" minWidth="-Infinity" SplitPane.resizableWithParent="false">
            <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" minWidth="60.0" style="&#10;" styleClass="label-text" text="Master" textAlignment="CENTER">
                <font>
                    <Font size="18.0" fx:id="x1" />
                </font>
                <textFill>
                    <Color blue="0.624" green="0.624" red="0.624" fx:id="x2" />
                </textFill>
            </Label>
            <VBox alignment="TOP_CENTER" layoutX="14.0" layoutY="191.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="60.0">
                <Button fx:id="btnShowListUsers" mnemonicParsing="false" styleClass="button-menu" text="СПИСОК ПОЛЬЗОВАТЕЛЕЙ" VBox.vgrow="ALWAYS">
                    <VBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0" />
                    </VBox.margin>
                </Button>
                <Button fx:id="btnShowListGroup" alignment="CENTER" mnemonicParsing="false" styleClass="button-menu" text="СПИСОК ГРУПП">
                    <VBox.margin>
                        <Insets left="5.0" right="5.0" top="10.0" />
                    </VBox.margin>
                </Button>
            </VBox>
        </AnchorPane>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <AnchorPane fx:id="usersPane" opacity="0.0">
                <Label alignment="CENTER" font="$x1" layoutX="14.0" layoutY="14.0" style="&#10;" styleClass="label-text" text="Информация" textAlignment="CENTER" textFill="$x2" />
                <TableView layoutX="235.0" layoutY="176.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
                    <columns>
                        <TableColumn prefWidth="75.0" text="ФИО" />
                        <TableColumn minWidth="-1.0" prefWidth="115.0" text="Дата рорждения" />
                        <TableColumn prefWidth="75.0" text="Chat Id" />
                        <TableColumn prefWidth="110.0" text="Группа" />
                        <TableColumn prefWidth="110.0" text="Статус" />
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                </TableView>
            </AnchorPane>
            <AnchorPane fx:id="groupsPane">
                <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" style="&#10;" styleClass="label-text" text="Информация" textAlignment="CENTER">
                    <font>
                        <Font size="18.0" fx:id="x11" />
                    </font>
                    <textFill>
                        <Color blue="0.624" green="0.624" red="0.624" fx:id="x21" />
                    </textFill>
                </Label>
                <Button fx:id="btnImportGroups" alignment="CENTER" contentDisplay="CENTER" layoutX="616.0" layoutY="8.0" maxWidth="-Infinity" mnemonicParsing="false" onAction="#importClick" prefHeight="30.0" styleClass="button-style1" AnchorPane.rightAnchor="14.0">
                    <graphic>
                        <ImageView fitHeight="25.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../../../img/import.png" />
                        </ImageView>
                    </graphic>
                </Button>
                <ListView fx:id="listGroups" layoutX="14.0" layoutY="86.0" prefHeight="475.0" prefWidth="200.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="70.0" />
            </AnchorPane>
        </GridPane>
    </SplitPane>
    <GridPane VBox.vgrow="SOMETIMES">
        <columnConstraints>
            <ColumnConstraints hgrow="NEVER" />
            <ColumnConstraints halignment="RIGHT" hgrow="ALWAYS" />
            <ColumnConstraints halignment="RIGHT" hgrow="NEVER" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints />
            <RowConstraints valignment="CENTER" />
        </rowConstraints>
        <Label maxHeight="1.7976931348623157E308" text="Left status" GridPane.rowIndex="1">
            <font>
                <Font size="11.0" fx:id="x3" />
            </font>
            <textFill>
                <Color blue="0.625" green="0.625" red="0.625" fx:id="x4" />
            </textFill>
        </Label>
        <Label fx:id="statusRightLb" alignment="TOP_LEFT" contentDisplay="RIGHT" text="Right status" GridPane.columnIndex="2" GridPane.rowIndex="1">
            <font>
                <Font size="11.0" fx:id="x22" />
            </font>
            <textFill>
                <Color blue="0.625" green="0.625" red="0.625" fx:id="x23" />
            </textFill>
        </Label>
        <ProgressBar fx:id="progrBarId" prefWidth="200.0" progress="0.0" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets right="20.0" />
            </GridPane.margin>
        </ProgressBar>
    </GridPane>
</VBox>
